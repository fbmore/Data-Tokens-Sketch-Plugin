{
	"bundleVersion": 1,
	"version": "0.9.1",
	"icon": "icon.png",
	"identifier": "com.fbmore.datatokens",
	"appcast": "https://raw.githubusercontent.com/fbmore/Data-Tokens-Sketch-Plugin/main/appcast.xml",
	"description": "Apply Data Tokens from a public GSheet URL or endpoint that returns a .tsv file",
	"homepage": "https://github.com/fbmore/Data-Tokens-Sketch-Plugin/",
	"author": "Francesco Bertocci",
	"authorEmail": "fbmore@gmail.com",
	"compatibleVersion": 80,
	"commands": [
		{
			"name": "Apply Data Tokens from URL",
			"identifier": "Import-Data-Url",
			"script": "import-data-url.js",
      		"shortcut": "control shift i"
	    },
		{
			"name": "Apply Data Tokens from Local Data Source",
			"identifier": "Import-Data-Local",
			"script": "import-data-local.js"
	    },
		{
			"name": "Define Remote Data Source (URL to .CSV or .TSV)",
			"identifier": "Define-Data",
			"script": "define-data.js"
		},
		{
			"name": "Define Local Data Source (Paste .CSV or .TSV)",
			"identifier": "Define-Data-Local",
			"script": "define-data-local.js"
		},
		{
			"name": "Export Data Tokens to TSV (from Selected Artboards or Page)",
			"identifier": "Export-Data-Tokens",
			"script": "export-data-tokens.js"
		},
		{
			"name": "Export Specific Data Tokens Reference Sheet",
			"identifier": "Export-Data-Tokens-Reference",
			"script": "export-data-tokens-reference.js"
		},
		{
			"name": "Insert Data Tokens (Text Layers)",
			"identifier": "Insert-Data-Tokens-Url",
			"script": "insert-data-tokens-url.js"
		},
		{
			"name": "Export All Data Tokens Reference Sheet",
			"identifier": "Export-All-Data-Tokens-Reference",
			"script": "export-all-data-tokens-reference.js"
		}
  	],
